{
  "name": "image-flatify",
  "version": "3.0.1",
  "description": "Take a directory, search images recursively and rename as single flat directory with date based filenames",
  "homepage": "https://github.com/paazmaya/image-flatify",
  "main": "index.js",
  "type": "module",
  "bin": {
    "image-flatify": "bin/image-flatify.js"
  },
  "scripts": {
    "lint": "eslint *.js bin lib",
    "test": "c8 --exclude tests tape tests/*_test.js tests/lib/*_test.js",
    "coverage": "c8 --exclude tests report --reporter=text-lcov > coverage.lcov"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "files": [
    "bin",
    "lib",
    "index.js",
    "npm-shrinkwrap.json",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "image",
    "rename",
    "collection",
    "organize"
  ],
  "author": {
    "name": "Juga Paazmaya",
    "email": "paazmaya@yahoo.com",
    "url": "https://paazmaya.fi"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paazmaya/image-flatify.git"
  },
  "bugs": {
    "url": "https://github.com/paazmaya/image-flatify/issues"
  },
  "license": "MIT",
  "dependencies": {
    "fecha": "4.2.3",
    "optionator": "0.9.4"
  },
  "devDependencies": {
    "c8": "9.1.0",
    "eslint": "8.57.0",
    "eslint-config-paazmaya": "9.0.1",
    "eslint-plugin-n": "17.12.0",
    "tape": "5.7.5"
  }
}
